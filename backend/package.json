    "jest": "^29.3.1",
    "nodemon": "^1.18.9",
    "pm2": "^3.2.4",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "ts-node": "7.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
