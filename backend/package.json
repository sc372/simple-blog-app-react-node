    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/helmet": "0.0.42",
    "@types/jest": "^23.3.9",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
